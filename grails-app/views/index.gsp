            <!-- Contact form -->
            <!-- Redo contact form with Grails / Groovy -->
            <form role="form" class="contact__form" id="form_sendemail">

                <div class="row">
                    <div class="col-sm-6">

                        <!-- Email -->
                        <div class="form-group">
                            <label for="email" class="sr-only">Your email address</label>
