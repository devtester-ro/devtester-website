<!-- HOME -->
<div class="hero hero_lg" id="home">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">

                <!-- Home heading -->
                <h1 style="font-size: 50px;" class="heading hero__heading animated fadeInUp">IMEI/ESN CHECK</h1>

                <!-- Home subheading -->
                <p style="font-size: 16px;" class="heading__sub hero-heading__sub animated fadeInUp delay_1">
                    The right place for your phone. Here you can find firmwares exactly for your device.
                </p>

            </div>
        </div>
        <!-- / .row -->
    </div>
    <!-- / .container -->
</div>
