<!-- GOOGLE ADS -->
<br>
<section class="ads-transparent" data-type="background" data-speed="1">
    <div class="color-overlay">
        <div class="container">
            <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
            <!-- DevTester-LG-IMEI-Top -->
            <ins class="adsbygoogle"
                 style="display:block"
                 data-ad-client="ca-pub-1881838157428798"
                 data-ad-slot="9877998269"
                 data-ad-format="auto"></ins>
            <script>
                (adsbygoogle = window.adsbygoogle || []).push({});
            </script>
        </div>
    </div>
</section>

<div class="section" id="description">

    <!-- ABOUT -->
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h4 class="heading">Get Firmwares - Info</h4>

                <p class="heading__sub">
                    Below you can find specific firmwares for your phone.
                </p>
            </div>
        </div>
        <!-- / .row -->
    </div>
    <!-- / .container -->


    <!-- FEATURES -->
    <div class="container">
        <div class="row">
            <div class="col-sm-4">
                <div class="features__item wow fadeIn">
                    <div class="features-item__icon">
                        <span class="oi oi-project"></span>
                    </div>

                    <div class="features-item__body">
                        <div class="features-item-body__title">
                            Latest Updates
                        </div>

                        <div class="features-item-body__summary">
                            This firmwares are always updated from LG Servers - <b>Last Update on: <g:formatDate
                                format="yyyy-MM-dd" date="${new Date()}"/><span>
                        </span></b>
                        </div>
                    </div>
                    <!-- / .features-item__body -->
                </div>
                <!-- / .features__item -->
            </div>

            <div class="col-sm-4">
                <div class="features__item wow fadeIn">
                    <div class="features-item__icon">
                        <span class="oi oi-code"></span>
                    </div>

                    <div class="features-item__body">
                        <div class="features-item-body__title">
                            Results per page
                        </div>

                        <div class="features-item-body__summary">
                            We are trying to give you as much as we can for every device to include more and more
                            firmwares.
                        </div>
                    </div>
                    <!-- / .features-item__body -->
                </div>
                <!-- / .features__item -->
            </div>

            <div class="col-sm-4">
                <div class="features__item wow fadeIn">
                    <div class="features-item__icon">
                        <span class="oi oi-envelope-open"></span>
                    </div>

                    <div class="features-item__body">
                        <div class="features-item-body__title">
                            Don't worry!
                        </div>

                        <div class="features-item-body__summary">
                            Every firmware that is displayed here is exactly what LG puts on your phone.
                        </div>
                    </div>
                    <!-- / .features-item__body -->
                </div>
                <!-- / .features__item -->
            </div>

        </div>
        <!-- / .row -->
    </div>
    <!-- / .container -->
</div>
<!-- / .section -->

<!-- ABOUT -->
<div class="section section_alt" id="download">
    <div class="container">
        <div class="row">
            <h3 id="get" class="heading">Get Firmwares</h3>
            <section class="ads-transparent" data-type="background" data-speed="1">
                <div class="color-overlay">
                    <div class="container">
                        <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                        <!-- DevTester-LG-IMEI-Middle -->
                        <ins class="adsbygoogle"
                             style="display:block"
                             data-ad-client="ca-pub-1881838157428798"
                             data-ad-slot="2354731469"
                             data-ad-format="auto"></ins>
                        <script>
                            (adsbygoogle = window.adsbygoogle || []).push({});
                        </script>
                    </div>
                </div>
            </section>
            <br>

            <div>
                <div class="table-responsive">
                    <table class='table table-inverse table-striped table-bordered table-hover' cellspacing='0'>
                        <thead>
                        <tr>
                            <th class='align'>MODEL</th>
                            <th class='align'>ESN</th>
                            <th class='align'>SUFFIX</th>
                            <th class='align'>SW VERSION</th>
                            <th class='align'>SW URL</th>
                        </tr>
                        </thead>
                        <g:each in="${imei}" var="thisModel">
                            <tbody>
                            <tr class='active'>
                                <td>${thisModel.model}</td>
                                <td>${thisModel.esn}</td>
                                <td>${thisModel.suffix}</td>
                                <td>${thisModel.sw_version}</td>
                                <td><a href="${thisModel.sw_url}">Download</a></td>
                            </tr>
                            </tbody>
                        </g:each>
                    </table>
                </div>
            </div>
        </div>
        <br>
    </div>

    <div class="text-center">
        <a style="border: solid;" href="/projects/lg-firmwares/"
           class="hero__btn btn btn-lg btn-primary animated fadeInUp delay_2">BACK</a>

        <div class="clearfix"></div>
    </div>
</div>
<!-- / .row -->

<g:render template="imei"/>

